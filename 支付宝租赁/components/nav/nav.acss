.nav{
  border-top: 1rpx solid #f2f2f2;
  position: fixed;
  bottom: 0;
  width: 100%;
  z-index: 5;
}
.nav image {
  display: inline-block;
  width: 44rpx;
  height: 44rpx;
}
.nav > view {
  flex: 1;
  text-align: center;
  font-size: 20rpx;
  box-sizing: border-box;
  padding: 10rpx;
}
.nav .shops {
  display: inline-block;
  width: 36rpx;
  height: 44rpx;
}