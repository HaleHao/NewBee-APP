/* pages/hosting/myhosting/index/index.wxss */
page{
  padding-bottom: 100rpx
}
.navorder {
  height: 84rpx;
  line-height: 84rpx;
}
.navorder > view {
  color: #666;
  font-size: 32rpx;
}
.navorder > view:nth-of-type(1) {
  margin-right: 100rpx;
}
.navorder .selected {
  color: #000;
  font-weight: bold;
}


.box {
  border-radius: 20rpx;
}

.goods_img {
  width: 200rpx;
  height: 200rpx;
  margin: 20rpx;
  border-radius: 20rpx;
}

.goods_title {
  font-size: 30rpx;
  width: 135px;
}
.state {
  color: #aeaeae;
}

.btn {
  border-radius: 30rpx;
  padding: 10rpx 20rpx;
  display: inline-block;
}