<van-tabbar active="{{ active }}" active-color="#4EA9F9" bind:change="onChange">
  <van-tabbar-item>
    <image slot="icon" src="{{ icon.index }}" mode="aspectFit" />
    <image slot="icon-active" src="{{ icon.indexs }}" mode="aspectFit" /> 首页
  </van-tabbar-item>
  <van-tabbar-item>
    <image slot="icon" src="{{ icon.shop }}" mode="aspectFit" />
    <image slot="icon-active" src="{{ icon.shops }}" mode="aspectFit" /> 门店
  </van-tabbar-item>
  <van-tabbar-item>
    <image slot="icon" src="{{ icon.order }}" mode="aspectFit" />
    <image slot="icon-active" src="{{ icon.orders }}" mode="aspectFit" /> 订单
  </van-tabbar-item>
  <van-tabbar-item>
    <image slot="icon" src="{{ icon.my }}" mode="aspectFit" />
    <image slot="icon-active" src="{{ icon.mys }}" mode="aspectFit" /> 我的
  </van-tabbar-item>
</van-tabbar>
